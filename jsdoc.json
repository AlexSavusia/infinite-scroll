{
  "source": {
    "include": ["src"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.(js|vue)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown",
    "jsdoc-vuejs"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
